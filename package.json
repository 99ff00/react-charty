{
  "name": "react-charty",
  "version": "0.1.7",
  "description": "To visualize your precious data",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "types": "dist/index.d.ts",
  "engines": {
    "node": ">=14.18",
    "npm": ">=6"
  },
  "scripts": {
    "test": "vitest run --passWithNoTests --environment jsdom",
    "test:watch": "vitest --passWithNoTests --environment jsdom",
    "build:css": "cleancss -o ./dist/charty.min.css ./src/styles.css",
    "build:standalone": "rollup -c rollup.standalone.mjs",
    "build:types": "tsc -p tsconfig.types.json",
    "build": "cleancss -o ./dist/charty.min.css ./src/styles.css && rollup -c rollup.standalone.mjs && tsc -p tsconfig.types.json && rollup -c rollup.config.mjs --forceExit",
    "start": "rollup -c rollup.config.mjs -w",
    "prepare": "yarn build",
    "predeploy": "yarn --cwd example install && yarn --cwd example build",
    "deploy": "yarn --cwd example deploy"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/99ff00/react-charty"
  },
  "keywords": [
    "chart",
    "react",
    "reactjs",
    "react-component",
    "javascript",
    "pie chart",
    "area chart",
    "line chart",
    "percentage",
    "canvas",
    "canvas2d"
  ],
  "author": "Aleksandr Telegin",
  "license": "MIT",
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0"
  },
  "devDependencies": {
    "@rollup/plugin-commonjs": "^25.0.0",
    "@rollup/plugin-node-resolve": "^15.0.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^11.0.0",
    "@rollup/plugin-url": "^8.0.0",
    "@types/prop-types": "^15.7.11",
    "@types/react": "^19.0.0",
    "@types/react-dom": "^19.0.0",
    "clean-css-cli": "^5.6.2",
    "jsdom": "^27.4.0",
    "postcss": "^8.4.38",
    "prop-types": "^15.7.2",
    "react": "^19.0.0",
    "react-dom": "^19.0.0",
    "rollup": "^4.40.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.2",
    "terser": "^5.19.0",
    "tslib": "^2.6.2",
    "typescript": "^5.4.0",
    "vitest": "^2.1.0"
  },
  "unpkg": "dist/charty.min.js",
  "files": [
    "dist"
  ]
}
