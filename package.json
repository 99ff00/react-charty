{
  "name": "react-charty",
  "version": "0.1.6",
  "description": "To visualize your precious data",
  "main": "dist/index.js",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts test --env=jsdom --passWithNoTests",
    "test:watch": "react-scripts test --env=jsdom",
    "build": "cleancss -o ./dist/charty.min.css ./src/styles.css ; tac ./src/charty.js | sed 1,2d | tac | sed 7,12d | terser -c --comments all -m -o ./dist/charty.min.js ; rollup -c",
    "start": "rollup -c -w",
    "prepare": "npm run build",
    "predeploy": "cd example && npm install && npm run build",
    "deploy": "gh-pages -d example/build"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/99ff00/react-charty"
  },
  "keywords": [
    "chart",
    "react",
    "reactjs",
    "react-component",
    "javascript",
    "pie",
    "area",
    "line",
    "percentage",
    "canvas",
    "canvas2d"
  ],
  "author": "Aleksandr Telegin",
  "license": "MIT",
  "peerDependencies": {
    "prop-types": "^15.7.2",
    "react": "^16.3.0",
    "react-dom": "^16.3.0"
  },
  "devDependencies": {
    "babel-core": "^6.26.3",
    "babel-plugin-external-helpers": "^6.22.0",
    "babel-preset-env": "^1.7.0",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-0": "^6.24.1",
    "cross-env": "^7.0.3",
    "eslint-config-standard": "^16.0.3",
    "eslint-config-standard-react": "^11.0.1",
    "eslint-plugin-import": "^2.23.4",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-promise": "^5.1.0",
    "eslint-plugin-react": "^7.24.0",
    "eslint-plugin-standard": "^4.1.0",
    "gh-pages": "^3.2.3",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "react-scripts": "^4.0.3",
    "rollup": "^2.56.0",
    "rollup-plugin-babel": "^3.0.7",
    "rollup-plugin-commonjs": "^9.1.3",
    "rollup-plugin-node-resolve": "^3.3.0",
    "rollup-plugin-peer-deps-external": "^2.2.4",
    "rollup-plugin-postcss": "^4.0.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-url": "^1.4.0",
    "terser": "^5.7.1"
  },
  "unpkg": "dist/charty.min.js",
  "files": [
    "dist"
  ]
}
